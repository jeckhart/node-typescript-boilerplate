{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "no-console": [
      true,
      "time",
      "timeEnd",
      "trace"
    ],
    "no-string-literal": false,
    "no-use-before-declare": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [true, "never"],
    "trailing-comma": [false, {"multiline": "always", "singleline": "never"}],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ]
  }
}
